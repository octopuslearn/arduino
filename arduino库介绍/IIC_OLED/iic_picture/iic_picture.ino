void setup()
{
  OLED_dispaly.begin(SSD1306_SWITCHCAPVCC, 0x3C);   //初始化OLED,IICd地址0x3c  //switchvcc： OLED的电压
}



void loop()
{
  OLED_dispaly.clearDisplay();      //清屏
  
  OLED_dispaly.drawBitmap(0, 0, Bim_128x64, 128, 64, SSD1306_WHITE );//左、右上角x坐标，图形数据，图形宽、高，颜色SSD1306_BLACK, SSD1306_WHITE or SSD1306_INVERT
  
